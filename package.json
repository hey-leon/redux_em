{
  "name": "redux-em",
  "version": "2.0.2",
  "description": "A set of helpers to dry up redux code, and provide a more opinionated api",
  "main": "umd/index.js",
  "scripts": {
    "build": "./tools/build/script.sh",
    "test": "./tools/test/watch.sh",
    "test:once": "./tools/test/once.sh",
    "coverage": "./tools/test/coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/leonp1991/redux_em.git"
  },
  "keywords": [
    "redux",
    "extension",
    "helpers"
  ],
  "author": "Leon Pearce <leonp1991@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/leonp1991/redux_em/issues"
  },
  "homepage": "https://github.com/leonp1991/redux_em#readme",
  "peerDependencies": {
    "redux": "^3.6.0"
  },
  "ava": {
    "files": [
      "tests/**/*.spec.js"
    ],
    "source": [
      "source/**/*.js"
    ],
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "devDependencies": {
    "ava": "latest",
    "babel-cli": "^6.24.1",
    "babel-core": "latest",
    "babel-eslint": "latest",
    "babel-loader": "latest",
    "babel-plugin-transform-flow-strip-types": "latest",
    "babel-plugin-transform-object-rest-spread": "latest",
    "babel-preset-env": "latest",
    "babel-register": "latest",
    "codecov": "latest",
    "eslint": "latest",
    "eslint-loader": "latest",
    "eslint-plugin-flowtype": "latest",
    "flow-bin": "latest",
    "nyc": "latest",
    "sinon": "latest",
    "webpack": "latest"
  }
}
